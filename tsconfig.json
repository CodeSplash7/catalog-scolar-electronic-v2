{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@mongodb/*": ["./src/lib/mongo/*"],
      "@edgestore/*": ["./src/lib/edgestore/*"],
      "@client-utils/*": ["./src/client-utils/*"],
      "@server-utils/*": ["./src/server-utils/*"],
      "@general-utils/*": ["./src/general-utils/*"],
      "@pages/*": ["./src/page-components"],
      "@loading/*": ["./src/loading"],
      "@layouts/*": ["./src/layouts"],
      "@types/*": ["./src/types"]
    },
    "target": "es2015"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/blogs/[year]/[month]/[day]/[blogName]/loading.tsx",
    "src/utils/allSides"
, "src/server-utils/password-functions.ts"  ],
  "exclude": ["node_modules"]
}
